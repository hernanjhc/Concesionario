@model IEnumerable<Concesionario.Models.Modelos>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MarcaId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.MarcaId) </td>
            <td>@Html.DisplayFor(modelItem => item.Modelo) </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MarcaId }) |
                @Html.ActionLink("Details", "Details", new { id = item.MarcaId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.MarcaId })
            </td>
        </tr>

    }
</table>