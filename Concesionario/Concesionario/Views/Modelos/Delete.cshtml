@model Concesionario.Models.Modelos
@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h1>¿Está seguro que desea eliminar @Model.Modelo?</h1>


@using (Html.BeginForm("Eliminar", "Modelos", new { id = Model.Id }, FormMethod.Post))
{
    <td colspan="2"><button type="submit" class="btn btn-primary">Eliminar</button></td>
}

@Html.ActionLink("Volver Atrás", "Index")