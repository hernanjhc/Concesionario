@model Concesionario.Models.Marcas

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm("Create", "Marcas", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(m => m.Marca, "Marca")
        @Html.TextBoxFor(m => m.Marca, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <td colspan="2"><button type="submit" class="btn btn-primary">Cargar</button></td>
    </div>
}

@Html.ActionLink("Volver al Listado", "Index")